pipeline {
    agent { node { label 'swarm' } }
    
    stages {
        stage("Test") {
            steps {
                sh 'whoami'
                sh 'pwd'
                sh 'hostname'
            }
        }
        stage("Test-ansible") {
            steps {
                sh 'pwd'
                sh 'cd ansible/playbooks'
                sh 'ls -la'
                sh 'hostname'
                
            }
        }
    }
}
